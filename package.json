{
  "name": "lerna-monorepo-starter",
  "description": "Lerna monorepo starter",
  "version": "1.0.0",
  "author": "Andrico Karoulla <karoulla.andrico@gmail.com>",
  "dependencies": {
    "lerna": "*",
    "gatsby": "*",
    "gatsby-link": "*",
    "gatsby-plugin-react-helmet": "*",
    "gatsby-plugin-react-next": "*",
    "gatsby-source-filesystem": "*",
    "gatsby-transformer-json": "*",
    "react": "*",
    "react-helmet": "*",
    "serve": "*",
    "styled-components": "*"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "test": "jest test --config ./json-config.json"
  },
  "scripts": {
    "run:blog": "cd packages/blog && yarn develop",
    "run:shop": "cd packages/shop && yarn develop",
    "run:all": "npm-run-all --parallel run:blog run:shop"
  },
  "devDependencies": {
    "@babel/preset-env": "*",
      "@babel/preset-react": "*",
    "babel-jest": "*",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ]
}
